{   
    "walking_movements":
    {
        "function_to_call": "IKWalk",
        "params_all_kinds":
            {
                "walk_forward":
                {
                    "x": 1,
                    "y": 0,
                    "theta": 0
                },
                "rotate_clockwise":
                {
                    "x": 0,
                    "y": 0,
                    "theta": 1
                },
                "rotate_counter_clockwise":
                {
                    "x": 0,
                    "y": 0,
                    "theta": -1
                },
                "stop_walk":
                {
                    "x": 0,
                    "y": 0,
                    "theta": 0
                }
            }
    },
    "head_movements":
    {
        "function_to_call": "XYHead",
        "params_all_kinds":
            {
                "head_to_left":
                {
                    "direction": "L",
                    "pattern": "Linear"
                },
                "head_to_right":
                {
                    "direction": "R",
                    "pattern": "Linear"
                },
                "head_to_up":
                {
                    "direction": "U",
                    "pattern": "Linear"
                },
                "head_to_down":
                {
                    "direction": "D",
                    "pattern": "Linear"
                },
                "head_to_center":
                {
                    "direction": "C",
                    "pattern": "Linear"
                },
                "head_search":
                {
                    "direction": "S",
                    "pattern": "Diagonal"
                },
                "stop_head":
                {
                    "direction": "N",
                    "pattern": "None"
                }
            }
    },
    "body_alignment_movs":
    {
        "function_to_call": "BAMove",
        "params_all_kinds":
            {
                "body_alignment_to_left":
                {
                    "head_movements": "head_to_right",
                    "walking_movements": "rotate_counter_clockwise"
                },
                "body_alignment_to_right":
                {
                    "head_movements": "head_to_left",
                    "walking_movements": "rotate_clockwise"
                },
                "stop_all_motions":
                {
                    "head_movements": "stop_head",
                    "walking_movements": "stop_walk"
                }
            }
    }
}